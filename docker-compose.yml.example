services:
  bot:
    build: .
    environment:
      BOT_TOKEN: 
      REDIS_HOST: 
      REDIS_PORT: 
    depends_on:
      - redis
    volumes:
      - ./app/services/docs_gen/templates:/app/app/services/docs_gen/templates

  redis:
    image: redis:5-alpine
    restart: on-failure
    volumes:
      - redis-data:/data

volumes:
  redis-data:


